#Vector

contoh_vector <- c(1,2,3)
contoh_vector

#menambahkan list angka otomatis
contoh_vector2 <- c(1:5)
contoh_vector2

#named vector atau vector yang memiliki nilai beberapa variable
contoh_vector3 <- c(channel="Creatif Dev", kelas="R Programming")
contoh_vector3["channel"]

#index pada vector

contoh_vector[1]